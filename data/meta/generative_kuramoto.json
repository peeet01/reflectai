  ],
  "parameters": {
    "N": "Csomópontok száma",
    "p": "Valószínűség él létrehozására",
    "K": "Kapcsolási erősség"
  },
  "applications": [
    "Hálózati dinamikák vizsgálata",
    "Szinkronizációs küszöbök elemzése",
    "Topológiai hatások Kuramoto szinkronizációra"
  ]
}
